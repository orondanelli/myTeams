  <body>
<section class="body">
<span class="separator"></span>
    <div class="col-lg-12">
	<section class="card">
		<header class="card-header">
			<div class="card-actions">
				<a href="#" class="card-action card-action-toggle" data-card-toggle></a>
				<a href="#" class="card-action card-action-dismiss" data-card-dismiss></a>
			</div>

			<h2 class="card-title">Crear Nivel</h2>
			<p class="card-subtitle">
				crear un  <code>nivel</code> de madurez.
			</p>
		</header>
		<div class="card-body">
			
			<form class="form-inline" action="/niveles/<%= JSON.parse(arrayLevels).id %>/update" method="post">
				<label class="sr-only" for="inlineFormInputName2">Nombre</label>
				<input type="text" class="form-control mb-2 mr-sm-2 mb-sm-0" id="inputLevelName" name="levelName" value="<%=JSON.parse(arrayLevels).levelName %>">
				<select class="form-control mb-2 mr-sm-2 mb-sm-0" name="selectPriorityLevel">
					<% if (JSON.parse(arrayLevels).levelpriority == 1) { %>
						<option value="1" selected >1</option>
					<% } else{ %>
						<option value="1">1</option>
					<% } %>
					<% if (JSON.parse(arrayLevels).levelpriority == 2) { %>
					<option value="2" selected >2</option>
					<% } else{ %>
					<option value="2">2</option>
					<% } %>
					<% if (JSON.parse(arrayLevels).levelpriority == 3) { %>
					<option value="3" selected >3</option>
					<% } else{ %>
					<option value="3">3</option>
					<% } %>
					<% if (JSON.parse(arrayLevels).levelpriority == 4) { %>
					<option value="4" selected >4</option>
					<% } else{ %>
					<option value="4">4</option>
					<% } %>
					</select>
                <input type="hidden" name="_method" value="PUT">
				<button type="submit" class="btn btn-primary">Enviar</button>
			</form>
		</div>
	</section>
</div>
</section>

